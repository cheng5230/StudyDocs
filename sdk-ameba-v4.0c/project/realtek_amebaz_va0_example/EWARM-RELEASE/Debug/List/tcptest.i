Exit1:
	lwip_close(iperf_data . client_fd);
Exit2:
	rtl_printf("\n\r%s: Close client socket",__func__);
	if(udp_client_buffer){
		vPortFree(udp_client_buffer);
		udp_client_buffer = 0;
	}
	return 0;
}

int udp_server_func(struct iperf_data_t iperf_data)
{
	struct sockaddr_in   ser_addr , client_addr;
	int                  addrlen = sizeof(struct sockaddr_in);
	int                  n = 1;
	uint32_t             start_time, report_start_time, end_time;
	int                  recv_size=0;
	uint64_t             total_size=0,report_size=0;
	struct iperf_udp_client_hdr client_hdr;
	uint8_t time_boundary = 0, size_boundary = 0;

	udp_server_buffer = pvPortMalloc(iperf_data.buf_size);
	if(!udp_server_buffer){
		rtl_printf("\n\r[ERROR] %s: Alloc buffer failed",__func__);
		goto Exit2;
	}

	
	if((iperf_data.server_fd = lwip_socket(2,2,17)) < 0){
		rtl_printf("\n\r[ERROR] %s: Create socket failed",__func__);
		goto Exit2;
	}
	rtl_printf("\n\r%s: Create socket fd = %d, port = %d", __func__,iperf_data.server_fd,iperf_data.port);

	lwip_setsockopt(iperf_data . server_fd,0xfff,0x0004,(const char *) &n,sizeof( n ));

	
	_memset(&ser_addr, 0, sizeof(ser_addr));
	ser_addr.sin_family = 2;
	ser_addr.sin_port = lwip_htons(iperf_data . port);
	ser_addr.sin_addr.s_addr = lwip_htonl(((u32_t)0x00000000UL));

	
	if( lwip_bind(iperf_data . server_fd,(struct sockaddr*)&ser_addr,sizeof(ser_addr)) < 0){
		rtl_printf("\n\r[ERROR] %s: Bind socket failed",__func__);
		goto Exit1;
	}

	rtl_printf("\n\r%s: Bind socket successfully",__func__);

	
	recv_size = lwip_recvfrom(iperf_data . server_fd,udp_server_buffer,iperf_data . buf_size,0,(struct sockaddr *) &client_addr,&addrlen);
	total_size+=recv_size;
	report_size+=recv_size;
	start_time = xTaskGetTickCount();
	report_start_time = start_time;
	end_time = start_time;
	if(!g_udp_bidirection){
		
		_memcpy(&client_hdr, udp_server_buffer, sizeof(client_hdr));
		if(client_hdr.mAmount != 0){
			client_hdr.mAmount = lwip_ntohl(client_hdr . mAmount);
			if(client_hdr.mAmount > 0x7fffffff){
				client_hdr.mAmount = (~(client_hdr.mAmount) + 1) / 100;
				time_boundary = 1;
				udp_client_data.time = client_hdr.mAmount;
			}else{
				size_boundary = 1;
				udp_client_data.total_size = client_hdr.mAmount;
			}
		}
		if(lwip_ntohl(client_hdr . flags) == 0x80000001){
			if(0 == g_udp_client_task){
				if(xTaskGenericCreate( ( udp_client_handler ), ( "udp_client_handler" ), ( 256 ), ( 0 ), ( ( ( UBaseType_t ) 0U ) + 1 + ( 4 ) ), ( &g_udp_client_task ), ( 0 ), ( 0 ) ) != ( ( ( BaseType_t ) 1 ) ))
					rtl_printf("\r\nUDP ERROR: Create UDP client task failed.");
				else{
					_strncpy(udp_client_data . server_ip, ipaddr_ntoa((ip_addr_t*)&(client_addr . sin_addr)), (prvStrLen((const unsigned char *) ipaddr_ntoa((ip_addr_t*)&(client_addr . sin_addr)))));
					udp_client_data.port = lwip_ntohl(client_hdr . mPort);
					udp_client_data.bandwidth = lwip_ntohl(client_hdr . mWinband);
					udp_client_data.buf_size = 1460;
					udp_client_data.tos_value = 96;
					udp_client_data.report_interval = 0xffffffff;
				}
			}
		}
	}else{
		if(iperf_data.total_size){
			client_hdr.mAmount = iperf_data.total_size;
			size_boundary = 1;
		}
		else if(iperf_data.time){
			client_hdr.mAmount = iperf_data.time;
			time_boundary = 1;
		}
	}

	if(time_boundary){
		while ( ((end_time - start_time) <= (( ( uint32_t ) 1000 ) * client_hdr.mAmount )) && (!g_udp_terminate) ) {
			recv_size = lwip_recvfrom(iperf_data . server_fd,udp_server_buffer,iperf_data . buf_size,0,(struct sockaddr *) &client_addr,&addrlen);
			if( recv_size < 0){
				rtl_printf("\n\r[ERROR] %s: Receive data failed",__func__);
				goto Exit1;
			}

			
			
			

			end_time = xTaskGetTickCount();
			total_size+=recv_size;
			report_size+=recv_size;
			if( (iperf_data.report_interval != 0xffffffff) && ((end_time - report_start_time) >= (( ( uint32_t ) 1000 ) * iperf_data.report_interval)) && ((end_time - report_start_time) <= (( ( uint32_t ) 1000 ) * (iperf_data.report_interval + 1)))) {
				rtl_printf("\n\r%s: Receive %d KBytes in %d ms, %d Kbits/sec",__func__,(uint32_t) (report_size/1024),(uint32_t)(end_time-report_start_time),((uint32_t)(report_size*8)/(end_time - report_start_time)));
				report_start_time = end_time;
				report_size = 0;
			}
		}
	}else if(size_boundary){
		while ( (total_size < client_hdr.mAmount) && (!g_udp_terminate) ) {
			recv_size = lwip_recvfrom(iperf_data . server_fd,udp_server_buffer,iperf_data . buf_size,0,(struct sockaddr *) &client_addr,&addrlen);
			if( recv_size < 0){
				rtl_printf("\n\r[ERROR] %s: Receive data failed",__func__);
				goto Exit1;
			}

			
			
			

			end_time = xTaskGetTickCount();
			total_size+=recv_size;
			report_size+=recv_size;
			if( (iperf_data.report_interval != 0xffffffff) && ((end_time - report_start_time) >= (( ( uint32_t ) 1000 ) * iperf_data.report_interval)) && ((end_time - report_start_time) <= (( ( uint32_t ) 1000 ) * (iperf_data.report_interval + 1)))) {
				rtl_printf("\n\r%s: Receive %d KBytes in %d ms, %d Kbits/sec",__func__,(uint32_t) (report_size/1024),(uint32_t)(end_time-report_start_time),((uint32_t)(report_size*8)/(end_time - report_start_time)));
				report_start_time = end_time;
				report_size = 0;
			}
		}
	}
	rtl_printf("\n\r%s: [END] Totally receive %d KBytes in %d ms, %d Kbits/sec",__func__,(uint32_t) (total_size/1024),(uint32_t)(end_time-start_time),((uint32_t)(total_size*8)/(end_time - start_time)));

Exit1:
	
	lwip_close(iperf_data . server_fd);

Exit2:
	if(udp_server_buffer){
		vPortFree(udp_server_buffer);
		udp_server_buffer = 0;
	}
	return 0;
}

