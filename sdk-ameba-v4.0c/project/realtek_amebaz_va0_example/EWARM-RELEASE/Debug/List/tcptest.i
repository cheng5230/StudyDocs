				if(argc < (argv_count+1))
					goto Exit;
				if(udp_server_data.start)
					udp_server_data.buf_size = prvAtoi(argv[argv_count]);
				else if(udp_client_data.start)
					udp_client_data.buf_size = prvAtoi(argv[argv_count]);
				else
					goto Exit;
				argv_count+=2;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-n") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				size_boundary = 1;
				if(udp_server_data.start)
					udp_server_data.total_size = km_parser(argv[argv_count],prvStrLen((const unsigned char *) argv[argv_count]));
				else if(udp_client_data.start)
					udp_client_data.total_size = km_parser(argv[argv_count],prvStrLen((const unsigned char *) argv[argv_count]));
				else
					goto Exit;
				argv_count+=2;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-p") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				if(udp_server_data.start)
					udp_server_data.port = (uint16_t) prvAtoi(argv[argv_count]);
				else if(udp_client_data.start)
					udp_client_data.port = (uint16_t) prvAtoi(argv[argv_count]);
				else
					goto Exit;
				argv_count+=2;
			}

			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-S") == 0){ 
				if(argc < (argv_count+1))
					goto Exit;
				if(udp_client_data.start){
					if(prvAtoi(argv[argv_count]) >= 0 && prvAtoi(argv[argv_count]) <= 255){
						tos_value = (uint8_t) prvAtoi(argv[argv_count]);
						wext_set_tos_value("wlan0", &tos_value);
						udp_client_data.tos_value = tos_value;
					}
					else
						goto Exit;
				}
				else
					goto Exit;
				argv_count+=2;
			}

			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-t") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				time_boundary = 1;
				if(udp_server_data.start)
					udp_server_data.time = prvAtoi(argv[argv_count]);
				else if(udp_client_data.start)
					udp_client_data.time = prvAtoi(argv[argv_count]);
                		else
					goto Exit;
				argv_count+=2;
			}
			else{
				goto Exit;
			}
		}
	}

	if(g_udp_bidirection == 1){
		udp_server_data.start = 1;
		udp_server_data.port = udp_client_data.port;
		udp_server_data.time = udp_client_data.time;
		udp_server_data.total_size = udp_client_data.total_size;
	}

	if(udp_server_data.start && (0 == g_udp_server_task)){
		if(xTaskGenericCreate( ( udp_server_handler ), ( "udp_server_handler" ), ( 256 ), ( 0 ), ( ( ( UBaseType_t ) 0U ) + 2 + ( 4 ) ), ( &g_udp_server_task ), ( 0 ), ( 0 ) ) != ( ( ( BaseType_t ) 1 ) ))
			rtl_printf("\r\nUDP ERROR: Create UDP server task failed.");
		else{
			if(udp_server_data.port == 0)
				udp_server_data.port = 5001;
			if(udp_server_data.buf_size == 0)
				udp_server_data.buf_size = 1500;
			if(udp_server_data.report_interval == 0)
				udp_server_data.report_interval = 0xffffffff;
			if((time_boundary == 0) && (size_boundary == 0)){
				udp_server_data.time = 10;
			}
		}
	}
	
	if(udp_client_data.start && (0 == g_udp_client_task)){
		if(xTaskGenericCreate( ( udp_client_handler ), ( "udp_client_handler" ), ( 256 ), ( 0 ), ( ( ( UBaseType_t ) 0U ) + 1 + ( 4 ) ), ( &g_udp_client_task ), ( 0 ), ( 0 ) ) != ( ( ( BaseType_t ) 1 ) ))
			rtl_printf("\r\nUDP ERROR: Create UDP client task failed.");
		else{
			if(udp_client_data.port == 0)
				udp_client_data.port = 5001;
			if(udp_client_data.bandwidth == 0)
				udp_client_data.bandwidth = 131072;
			if(udp_client_data.buf_size == 0)
				udp_client_data.buf_size = 1460;
			if(udp_client_data.tos_value == 0)
				udp_client_data.tos_value = 96;
			if(udp_client_data.report_interval == 0)
				udp_client_data.report_interval = 0xffffffff;
			if((time_boundary == 0) && (size_boundary == 0)){
				udp_client_data.time = 10;
			}
		}
	}

	return;

Exit:
	rtl_printf("\n\r[ATWU] Command format ERROR!\n");
	rtl_printf("\n\r[ATWU] Usage: ATWU=[-s|-c,host|stop][options]\n");
	rtl_printf("\n\r   Client/Server:\n");
	rtl_printf("  \r     stop           terminate client & server\n");
	rtl_printf("  \r     -i    #        seconds between periodic bandwidth reports\n");
	rtl_printf("  \r     -l    #        length of buffer to read or write (default 1460 Bytes)\n");
	rtl_printf("  \r     -p    #        server port to listen on/connect to (default 5001)\n");
	rtl_printf("\n\r   Server specific:\n");
	rtl_printf("  \r     -s             run in server mode\n");
	rtl_printf("\n\r   Client specific:\n");
	rtl_printf("  \r     -b    #[KM]    for UDP, bandwidth to send at in bits/sec (default 1 Mbit/sec)\n");
	rtl_printf("  \r     -c    <host>   run in client mode, connecting to <host>\n");
	rtl_printf("  \r     -d             Do a bidirectional test simultaneously\n");
	rtl_printf("  \r     -t    #        time in seconds to transmit for (default 10 secs)\n");
	rtl_printf("  \r     -n    #[KM]    number of bytes to transmit (instead of -t)\n");

	rtl_printf("  \r     -S    #        set the IP 'type of service'\n");

	rtl_printf("\n\r   Example:\n");
	rtl_printf("  \r     ATWU=-s,-p,5002\n");
	rtl_printf("  \r     ATWU=-c,192.168.1.2,-t,100,-p,5002\n");
	return;
}
