int SC_send_simple_config_ack(u8 round)
{

	int ack_transmit_round, ack_num_each_sec;
	int ack_socket;
	
	struct sockaddr_in to_addr;

	u8 check_phone_ack = 0;

	SC_set_ack_content();
	
	ack_socket = lwip_socket(2,2,17);
	if (ack_socket == -1) {
		return 	-1;
	}

	else {
		struct sockaddr_in bindAddr;
		bindAddr.sin_family = 2;
		bindAddr.sin_port = lwip_htons(8864);
		bindAddr.sin_addr.s_addr = ((u32_t)0x00000000UL);
		if(lwip_bind(ack_socket,(struct sockaddr *) &bindAddr,sizeof(bindAddr)) == 0)
			check_phone_ack = 1;
	}

	rtl_printf("\nSending simple config ack\n");
	_memset((void*)(&to_addr), 0, sizeof(*(&to_addr)));
	to_addr.sin_family = 2;
	to_addr.sin_port = lwip_htons(8864);
	to_addr.sin_addr.s_addr = (backup_sc_ctx->ip_addr);
	for (ack_transmit_round = 0;ack_transmit_round < round; ack_transmit_round++) {
		for (ack_num_each_sec = 0;ack_num_each_sec < 20; ack_num_each_sec++) {
			
			lwip_sendto(ack_socket,(unsigned char *)ack_content,sizeof(struct ack_msg),0,(struct sockaddr *) &to_addr,sizeof(struct sockaddr));
			
			vTaskDelay(50);	 


			if(check_phone_ack) {
				unsigned char packet[100];
				int packetLen;
				struct sockaddr from;
				struct sockaddr_in *from_sin = (struct sockaddr_in*) &from;
				socklen_t fromLen = sizeof(from);

				if((packetLen = lwip_recvfrom(ack_socket,&packet,sizeof(packet),0x08,&from,&fromLen)) >= 0) {
					uint8_t *ip = (uint8_t *) &from_sin->sin_addr.s_addr;
					uint16_t from_port = lwip_ntohs(from_sin->sin_port);
					rtl_printf("recv %d bytes from %d.%d.%d.%d:%d at round=%d, num=%d\n",
							packetLen, ip[0], ip[1], ip[2], ip[3], from_port,
							ack_transmit_round, ack_num_each_sec);
					goto leave_ack;
				}
			}

		}
	}

leave_ack:
	lwip_close(ack_socket);








	return 0;
}



static int  SC_check_and_show_connection_info(void)
{
	rtw_wifi_setting_t setting;	
	

    int ret = -1;
	int retry = 0; 

    vTaskPrioritySet(0, ( ( UBaseType_t ) 0U ) + 3);	
	while(retry < 2)
	{
    	 
    	 
    	ret = LwIP_DHCP(0, DHCP_START);
	    if (ret == DHCP_ADDRESS_ASSIGNED)
	        break;
	    else
	        retry++;    	
	}
    vTaskPrioritySet(0, ( ( UBaseType_t ) 0U ) + 1);

	

	wifi_get_setting("wlan0", &setting);
	wifi_show_setting("wlan0", &setting);

	

	if (ret != DHCP_ADDRESS_ASSIGNED)
		return SC_DHCP_FAIL;
	else

		return SC_SUCCESS;

}

