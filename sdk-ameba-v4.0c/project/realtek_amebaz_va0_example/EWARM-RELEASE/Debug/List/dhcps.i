		dhcps_pcb = 0;	
	}
	if (dhcps_ip_table_semaphore != 0) {	
		vQueueDelete( ( QueueHandle_t ) ( dhcps_ip_table_semaphore ) );
		dhcps_ip_table_semaphore = 0;
	}		
}
