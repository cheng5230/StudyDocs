	}

	 
	_memset(scan_buf . buf, 0, scan_buf . buf_len);
	if(ssid && ssid_len > 0 && ssid_len <= 32){
		_memcpy(scan_buf . buf, &ssid_len, sizeof(int));
		_memcpy(scan_buf . buf+sizeof(int), ssid, ssid_len);
