static int SC_softAP_find_ap_from_scan_buf(char*buf, int buflen, char *target_ssid, void *user_data)
{
	rtw_wifi_setting_t *pwifi = (rtw_wifi_setting_t *)user_data;
	int plen = 0;
	
	while(plen < buflen){
		u8 len, ssid_len, security_mode;
		char *ssid;

		
		len = (int)*(buf + plen);
		
		if(len == 0) break;
		
		ssid_len = len - 14;
		ssid = buf + plen + 14 ;
		if((ssid_len == prvStrLen((const unsigned char *) target_ssid))
			&& (!_memcmp(ssid, target_ssid, ssid_len)))
		{
			_strcpy((char*)pwifi->ssid, target_ssid);
			
			pwifi->channel = *(buf + plen + 13);
			
			security_mode = (u8)*(buf + plen + 11);
			if(security_mode == 0)
				pwifi->security_type = RTW_SECURITY_OPEN;
			else if(security_mode == 1)
				pwifi->security_type = RTW_SECURITY_WEP_PSK;
			else if(security_mode == 3)
				pwifi->security_type = RTW_SECURITY_WPA2_AES_PSK;
			break;
		}
		plen += len;
	}
	return 0;
}

