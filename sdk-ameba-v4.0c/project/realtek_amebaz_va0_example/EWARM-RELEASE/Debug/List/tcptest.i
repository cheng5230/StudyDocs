uint64_t km_parser(char *buf, int len)
{
	uint64_t ret=0;
	int keyword_num=0;
	char num_str[17] = "\0";
	uint64_t num;

	if(len>16)
		return ret;

	while((buf[keyword_num] != '\0')&&(keyword_num<len)){
		if((buf[keyword_num] == 'k')||(buf[keyword_num] == 'K')){
			_strncpy(num_str, buf, keyword_num);
			num = simple_strtol(num_str, 0, 10);
			ret = num * 1024;
			break;
		}
		else if((buf[keyword_num] == 'm')||(buf[keyword_num] == 'M')){
			_strncpy(num_str, buf, keyword_num);
			num = simple_strtol(num_str, 0, 10);
			ret = num * 1048576;
			break;
		}
		keyword_num++;
		if(keyword_num == len){
			_strncpy(num_str, buf, keyword_num);
			num = simple_strtol(num_str, 0, 10);
			ret = num;
			break;
		}

	}
	return ret;
}

void cmd_tcp(int argc, char **argv)
{
	int argv_count = 2;
	uint8_t time_boundary = 0, size_boundary = 0;

	if(argc < 2)
		goto Exit;

	g_tcp_terminate = 0;

	while(argv_count<=argc){
		
		if(argv_count == 2){
			if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-s") == 0){
				if(g_tcp_server_task){
					rtl_printf("\n\rTCP: TCP Server is already running.");
					return;
				}else{
					_memset(&tcp_server_data, 0, sizeof(struct iperf_data_t));
					tcp_server_data.start = 1;
					argv_count++;
				}
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "stop") == 0){
				if(argc == 2){
					vTaskDelay(100);
					g_tcp_terminate = 1;
					tcp_server_data.start = 0;
					tcp_client_data.start = 0;

					if(g_tcp_server_task){
						if(tcp_server_data.server_fd >=0){
							lwip_close(tcp_server_data . server_fd);
						}
						if(tcp_server_data.client_fd >=0){
							lwip_close(tcp_server_data . client_fd);
						}
						if(tcp_server_buffer){
							vPortFree(tcp_server_buffer);
							tcp_server_buffer = 0;
						}
						rtl_printf("\n\rTCP server stopped!\n");
						vTaskDelete(g_tcp_server_task);
						g_tcp_server_task = 0;
					}

					return;
				}
				else
					goto Exit;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-c") == 0){
				if(g_tcp_client_task){
					rtl_printf("\n\rTCP: TCP client is already running. Please enter \"ATWT=stop\" to stop it.");
					return;
				}else{
					if(argc < (argv_count+1))
						goto Exit;
					_memset(&tcp_client_data, 0, sizeof(struct iperf_data_t));
					tcp_client_data.start = 1;
					_strncpy(tcp_client_data . server_ip, argv[2], (prvStrLen((const unsigned char *) argv[2])>16)?16:prvStrLen((const unsigned char *) argv[2]));
					argv_count+=2;
				}
			}
			else
				goto Exit;
		}
		else{
			if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-d") == 0){
				if(tcp_client_data.start)
					g_tcp_bidirection = 1;
				else
					goto Exit;
				argv_count+=1;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-i") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				if(tcp_server_data.start)
					tcp_server_data.report_interval = (uint32_t) prvAtoi(argv[argv_count]);
				else if(tcp_client_data.start)
					tcp_client_data.report_interval = (uint32_t) prvAtoi(argv[argv_count]);
				else
					goto Exit;
				argv_count+=2;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-l") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				if(tcp_server_data.start)
					tcp_server_data.buf_size = prvAtoi(argv[argv_count]);
				else if(tcp_client_data.start)
					tcp_client_data.buf_size = prvAtoi(argv[argv_count]);
				else
					goto Exit;
				argv_count+=2;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-n") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				size_boundary = 1;
				if(tcp_client_data.start)
					tcp_client_data.total_size = km_parser(argv[argv_count],prvStrLen((const unsigned char *) argv[argv_count]));
				else
					goto Exit;
				argv_count+=2;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-p") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				if(tcp_server_data.start)
					tcp_server_data.port = (uint16_t) prvAtoi(argv[argv_count]);
				else if(tcp_client_data.start)
					tcp_client_data.port = (uint16_t) prvAtoi(argv[argv_count]);
				else
					goto Exit;
				argv_count+=2;
			}
			else if(prvStrCmp((const unsigned char *) argv[argv_count-1], (const unsigned char *) "-t") == 0){
				if(argc < (argv_count+1))
					goto Exit;
				time_boundary = 1;
				if(tcp_client_data.start)
					tcp_client_data.time = prvAtoi(argv[argv_count]);
				else
					goto Exit;
				argv_count+=2;
			}
			else{
				goto Exit;
			}
		}
	}

	if(g_tcp_bidirection == 1){
		tcp_server_data.start = 1;
		tcp_server_data.port = tcp_client_data.port;
	}

	if(tcp_server_data.start && (0 == g_tcp_server_task)){
		if(xTaskGenericCreate( ( tcp_server_handler ), ( "tcp_server_handler" ), ( 256 ), ( 0 ), ( ( ( UBaseType_t ) 0U ) + 1 + ( 4 ) ), ( &g_tcp_server_task ), ( 0 ), ( 0 ) ) != ( ( ( BaseType_t ) 1 ) ))
			rtl_printf("\n\rTCP ERROR: Create TCP server task failed.");
		else{
			if(tcp_server_data.port == 0)
				tcp_server_data.port = 5001;
			if(tcp_server_data.buf_size == 0)
				tcp_server_data.buf_size = 1500;
			if(tcp_server_data.report_interval == 0)
				tcp_server_data.report_interval = 0xffffffff;
		}
	}

	if(tcp_client_data.start && (0 == g_tcp_client_task)){
		if(xTaskGenericCreate( ( tcp_client_handler ), ( "tcp_client_handler" ), ( 256 ), ( 0 ), ( ( ( UBaseType_t ) 0U ) + 1 + ( 4 ) ), ( &g_tcp_client_task ), ( 0 ), ( 0 ) ) != ( ( ( BaseType_t ) 1 ) ))
			rtl_printf("\n\rTCP ERROR: Create TCP client task failed.");
		else{
			if(tcp_client_data.port == 0)
				tcp_client_data.port = 5001;
			if(tcp_client_data.buf_size == 0)
				tcp_client_data.buf_size = 1460;
			if(tcp_client_data.report_interval == 0)
				tcp_client_data.report_interval = 0xffffffff;
			if((time_boundary == 0) && (size_boundary == 0))
				tcp_client_data.time = 10;
		}
	}

	return;
Exit:
	rtl_printf("\n\r[ATWT] Command format ERROR!\n");
	rtl_printf("\n\r[ATWT] Usage: ATWT=[-s|-c,host|stop],[options]\n");
	rtl_printf("\n\r   Client/Server:\n");
	rtl_printf("  \r     stop           terminate client & server\n");
	rtl_printf("  \r     -i    #        seconds between periodic bandwidth reports\n");
	rtl_printf("  \r     -l    #        length of buffer to read or write (default 1460 Bytes)\n");
	rtl_printf("  \r     -p    #        server port to listen on/connect to (default 5001)\n");
	rtl_printf("\n\r   Server specific:\n");
	rtl_printf("  \r     -s             run in server mode\n");
	rtl_printf("\n\r   Client specific:\n");
	rtl_printf("  \r     -c    <host>   run in client mode, connecting to <host>\n");
	rtl_printf("  \r     -d             Do a bidirectional test simultaneously\n");
	rtl_printf("  \r     -t    #        time in seconds to transmit for (default 10 secs)\n");
	rtl_printf("  \r     -n    #[KM]    number of bytes to transmit (instead of -t)\n");
	rtl_printf("\n\r   Example:\n");
	rtl_printf("  \r     ATWT=-s,-p,5002\n");
	rtl_printf("  \r     ATWT=-c,192.168.1.2,-t,100,-p,5002\n");
	return;
}

